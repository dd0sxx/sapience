{
  "name": "@sapience/ui",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/",
    "tailwind-preset.js"
  ],
  "scripts": {
    "dev": "tsup index.ts --format cjs,esm --dts --watch",
    "build": "tsup index.ts --format cjs,esm --dts --tsconfig tsconfig.build.json && pnpm build-storybook",
    "build:turbo": "turbo run build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && pnpm format",
    "type-check": "tsc --noEmit",
    "check:turbo": "npx turbo lint type-check",
    "test:e2e": "playwright test",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "prerelease": "pnpm lint && pnpm type-check && pnpm build",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "echo 'Skipping husky install'",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
