{
  "name": "@sapience/sdk",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "boilerplates/README.md",
    "ui/tailwind-preset.js"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./mcp/client": {
      "import": "./dist/mcp/client.mjs",
      "require": "./dist/mcp/client.js",
      "types": "./dist/mcp/client.d.ts"
    },
    "./abis": {
      "import": "./dist/abis/index.mjs",
      "require": "./dist/abis/index.js",
      "types": "./dist/abis/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.mjs",
      "require": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    },
    "./ui": {
      "import": "./dist/ui/index.mjs",
      "require": "./dist/ui/index.js",
      "types": "./dist/ui/index.d.ts"
    },
    "./queries": {
      "import": "./dist/queries/index.mjs",
      "require": "./dist/queries/index.js",
      "types": "./dist/queries/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./contracts": {
      "import": "./dist/contracts/index.mjs",
      "require": "./dist/contracts/index.js",
      "types": "./dist/contracts/index.d.ts"
    },
    "./boilerplates": "./boilerplates/README.md"
  },
  "scripts": {
    "prepare": "pnpm run build:lib",
    "dev": "tsup index.ts mcp/client.ts abis/index.ts constants/index.ts contracts/index.ts ui/index.ts queries/index.ts types/index.ts --format cjs,esm --dts --watch",
    "build:lib": "tsup index.ts mcp/client.ts abis/index.ts constants/index.ts contracts/index.ts ui/index.ts queries/index.ts types/index.ts --format cjs,esm --dts --tsconfig tsconfig.build.json",
    "build": "pnpm run build:lib && pnpm build-storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@tanstack/react-query": "5.x",
    "react": ">=18 || 19",
    "react-dom": ">=18 || 19",
    "react-hook-form": "7.53.0",
    "lucide-react": "0.461.0",
    "@uniswap/sdk-core": "5.4.0",
    "@uniswap/v3-core": "1.0.1",
    "@uniswap/v3-sdk": "3.14.0",
    "wagmi": "2.14.16",
    "viem": "2.23.5"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@radix-ui/react-accordion": "1.2.3",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-dialog": "1.1.6",
    "@radix-ui/react-dropdown-menu": "2.1.2",
    "@radix-ui/react-label": "2.1.2",
    "@radix-ui/react-popover": "1.1.6",
    "@radix-ui/react-scroll-area": "1.2.0",
    "@radix-ui/react-select": "2.1.6",
    "@radix-ui/react-separator": "1.1.0",
    "@radix-ui/react-slider": "1.2.3",
    "@radix-ui/react-slot": "1.1.2",
    "@radix-ui/react-switch": "1.1.1",
    "@radix-ui/react-tabs": "1.1.3",
    "@radix-ui/react-toast": "1.2.2",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.3",
    "abitype": "1.0.8",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "decimal.js": "10.5.0",
    "embla-carousel-auto-scroll": "8.6.0",
    "embla-carousel-react": "8.6.0",
    "graphql-request": "7.2.0",
    "lodash": "4.17.21",
    "react-day-picker": "8.10.1",
    "tailwind-merge": "2.5.4",
    "tailwindcss-animate": "1.0.7",
    "vaul": "1.1.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.3",
    "@commitlint/cz-commitlint": "18.6.1",
    "@eslint/js": "9.31.0",
    "@graphql-eslint/eslint-plugin": "4.4.0",
    "@next/eslint-plugin-next": "15.4.1",
    "@storybook/addon-docs": "9.1.2",
    "@storybook/addon-onboarding": "9.1.2",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-webpack5-compiler-swc": "3.0.0",
    "@storybook/react-webpack5": "9.1.2",
    "@types/lodash": "4.17.7",
    "@types/node": "22.14.0",
    "@types/react": "19.1.8",
    "autoprefixer": "10.4.20",
    "commitizen": "4.3.1",
    "cross-env": "7.0.3",
    "eslint": "9.31.0",
    "eslint-config-next": "14.2.12",
    "eslint-config-sznm": "2.0.3",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "9.1.2",
    "lint-staged": "15.2.10",
    "postcss": "8.5.3",
    "prettier": "3.3.3",
    "standard-version": "9.5.0",
    "storybook": "9.1.2",
    "tailwindcss": "3.4.14",
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.21.0",
    "react-hook-form": "7.53.0",
    "lucide-react": "0.461.0",
    "@uniswap/sdk-core": "5.4.0",
    "@uniswap/v3-core": "1.0.1",
    "@uniswap/v3-sdk": "3.14.0",
    "wagmi": "2.14.16",
    "viem": "2.23.5"
  },
  "engines": {
    "node": ">=20.14.x",
    "pnpm": "9.x"
  }
}
