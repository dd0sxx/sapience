{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "strictPropertyInitialization": false,
      "resolveJsonModule": true,
      "paths": {"@/protocol/*": ["../protocol/*"]}
    },
    "include": ["src"],
    "exclude": ["node_modules", "**/*.spec.ts"]
  }
  